            android:typeface="monospace" android:textColor="@color/titulo" android:textStyle="bold"
            tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="355dp"
            android:layout_height="71dp"
            android:inputType="textMultiLine"
