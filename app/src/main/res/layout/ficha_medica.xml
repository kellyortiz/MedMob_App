            android:text="Convênio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_Convenio"
            app:layout_constraintTop_toTopOf="@+id/myRectangleView14"
            app:layout_constraintStart_toEndOf="@+id/checkBox_SUS" android:layout_marginStart="68dp"/>
</android.support.constraint.ConstraintLayout>