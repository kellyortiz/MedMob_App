            android:text="Pressão"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="213dp"
            tools:layout_editor_absoluteX="30dp" android:id="@+id/pressaoTexto7" android:typeface="monospace"
            android:textColor="@color/TituloInformacao" tools:ignore="MissingConstraints"/>
