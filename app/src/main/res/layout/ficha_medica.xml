            android:layout_height="wrap_content" tools:layout_editor_absoluteY="476dp"
            tools:layout_editor_absoluteX="27dp" android:id="@+id/pressaoTexto6" android:typeface="monospace"
            android:textColor="@color/TituloInformacao" tools:ignore="MissingConstraints"/>
