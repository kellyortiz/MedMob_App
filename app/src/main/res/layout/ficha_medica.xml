        <string-array android:name="pressaoEscolha">
            <item>Normal</item>
            <item>Baixa</item>
            <item>Alta</item>
        </string-array>
    </resources>
    <View
            android:id="@+id/myRectangleView11"
            android:layout_width="358dp"
            android:layout_height="32dp"
            android:background="@drawable/retangulo" tools:layout_editor_absoluteY="313dp"
            tools:layout_editor_absoluteX="30dp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Diabete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="290dp"
            tools:layout_editor_absoluteX="30dp" android:id="@+id/pressaoTexto8" android:typeface="monospace"
            android:textColor="@color/TituloInformacao" tools:ignore="MissingConstraints"/>
    <View
            android:id="@+id/myRectangleView12"
            android:layout_width="358dp"
            android:layout_height="32dp"
            android:background="@drawable/retangulo" tools:layout_editor_absoluteY="242dp"
            tools:layout_editor_absoluteX="30dp" tools:ignore="MissingConstraints"/>
    <View
            android:id="@+id/myRectangleView13"
            android:layout_width="358dp"
            android:layout_height="52dp"
            android:background="@drawable/retangulo" tools:layout_editor_absoluteY="501dp"
            tools:layout_editor_absoluteX="27dp" tools:ignore="MissingConstraints"/>
