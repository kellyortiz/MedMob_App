            android:layout_height="wrap_content" tools:layout_editor_absoluteY="570dp"
            tools:layout_editor_absoluteX="27dp" android:id="@+id/pressaoTexto9" android:typeface="monospace"
            android:textColor="@color/TituloInformacao" tools:ignore="MissingConstraints"/>
