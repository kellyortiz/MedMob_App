            android:text="SUS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_SUS"
            app:layout_constraintTop_toTopOf="@+id/myRectangleView14"
            app:layout_constraintStart_toStartOf="@+id/myRectangleView14" android:layout_marginStart="8dp"/>
