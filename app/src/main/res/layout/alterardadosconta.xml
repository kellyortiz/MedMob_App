<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="414dp"
            android:layout_height="68dp"
            android:background="@color/quadrado" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" tools:ignore="MissingConstraints">
    </LinearLayout>
    <ImageView
            android:layout_width="442dp"
            android:layout_height="102dp" app:srcCompat="@drawable/logo_2" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="-15dp" android:id="@+id/imageView15"/>
    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true" app:srcCompat="@android:drawable/ic_menu_sort_by_size"
            tools:layout_editor_absoluteY="16dp" tools:layout_editor_absoluteX="16dp"
            android:id="@+id/floatingActionButton6" app:backgroundTint="@color/quadrado"
            app:rippleColor="@color/quadrado"/>
    <TextView
            android:text="Alterar dados da conta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="120dp"
            tools:layout_editor_absoluteX="80dp" android:id="@+id/text_AlterarDadosConta" android:textSize="18sp"
            android:typeface="monospace" android:textColor="@color/titulo" android:textStyle="bold"
            tools:ignore="MissingConstraints"/>
    <Button
            android:text="Salvar"
            android:layout_width="65dp"
            android:layout_height="23dp" tools:layout_editor_absoluteY="423dp"
            tools:layout_editor_absoluteX="316dp" android:id="@+id/salvarDadosLogin"
            android:textColor="@color/textoBotao"
            android:background="@drawable/botao_personalizado" android:backgroundTint="@color/colorteste"
            android:textSize="10sp" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Confirmar senha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="348dp"
            tools:layout_editor_absoluteX="32dp" android:id="@+id/textView28"
            android:textColor="@color/barraDeProgresso" android:typeface="monospace" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="342dp"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="367dp" tools:layout_editor_absoluteX="32dp" android:id="@+id/editText12"
            android:textSize="14sp" android:textColor="@color/colorAccent" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Senha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="264dp"
            tools:layout_editor_absoluteX="32dp" android:id="@+id/cadastro_senha2"
            android:textColor="@color/barraDeProgresso" android:typeface="monospace" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="343dp"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="282dp" tools:layout_editor_absoluteX="31dp"
            android:textSize="14sp" tools:ignore="MissingConstraints" android:id="@+id/editText11"/>
    <EditText
            android:layout_width="346dp"
            android:layout_height="37dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            tools:layout_editor_absoluteY="202dp" tools:layout_editor_absoluteX="32dp" android:id="@+id/editText10"
            android:textSize="12sp" android:textColor="@color/colorAccent" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="E-mail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="181dp"
            tools:layout_editor_absoluteX="32dp" android:id="@+id/textView27"
            android:textColor="@color/barraDeProgresso" android:typeface="monospace" tools:ignore="MissingConstraints"/>
</android.support.constraint.ConstraintLayout>